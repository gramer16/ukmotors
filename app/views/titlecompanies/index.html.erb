<h1>Listing titlecompanies</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Company description</th>
      <th>Address</th>
      <th>City</th>
      <th>Zipcode</th>
      <th>Contact name</th>
      <th>Company website</th>
      <th>Company phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @titlecompanies.each do |titlecompany| %>
      <tr>
        <td><%= titlecompany.company_name %></td>
        <td><%= titlecompany.company_description %></td>
        <td><%= titlecompany.address %></td>
        <td><%= titlecompany.city %></td>
        <td><%= titlecompany.zipcode %></td>
        <td><%= titlecompany.contact_name %></td>
        <td><%= titlecompany.company_website %></td>
        <td><%= titlecompany.company_phone %></td>
        <td><%= titlecompany.email %></td>
        <td><%= link_to 'Show', titlecompany %></td>
        <td><%= link_to 'Edit', edit_titlecompany_path(titlecompany) %></td>
        <td><%= link_to 'Destroy', titlecompany, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Titlecompany', new_titlecompany_path %>
